{
    "version": 2,
    "env": {
    },
    "builds": [
        {
          "src": "index.js",
          "use": "@vercel/node",
          "config": { "includeFiles": ["index.js"] }
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "index.js"
        }
    ]   
}